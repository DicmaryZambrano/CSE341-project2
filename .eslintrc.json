{
  "extends": [
    "airbnb",
    "prettier",
    "plugin:node/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "callback-return": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off",
    "consistent-return": "off"
  }
}